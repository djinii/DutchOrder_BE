<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dutchOrder.server.dao.C_MainDao">
	<select id="mapAddrList" parameterType="int" resultType="com.dutchOrder.server.model.Address">
		select mnum, adnum, adaddr
		from address
		where mnum = #{mnum};
	</select>
</mapper>